{
    "name": "vlauciani/laravel-validation-rules",
    "description": "Validation rules for Laravel.",
    "type": "project",
    "keywords": [
        "vlauciani",
        "validation",
        "rules",
        "laravel"
    ],
    "homepage": "https://github.com/vlauciani/laravel-validation-rules",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentino Lauciani",
            "email": "vlauciani@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "VLauciani\\LaravelValidationRules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VLauciani\\LaravelValidationRules\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VLauciani\\LaravelValidationRules\\ValidationRulesServiceProvider"
            ]
        }
    }
}
